@page
@using TorneoDeFutbol.App.Dominio
@model TorneoDeFutbol.App.Frontend.pages.Municipios.EditModel
@{
}
<h1>Editar Municipio</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="municipio.Id"></label>
        <input asp-for="municipio.Id" class="form-control" readonly>
    </div>
    <div class="form-group">
        <label asp-for="municipio.Nombre"></label>
        <input asp-for="municipio.Nombre" class="form-control">
        <span asp-validation-for="municipio.Nombre" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-secondary">Editar Municipio</button>
</form>
<a class="btn btn-primary" asp-page="./Index">Cancelar</a>