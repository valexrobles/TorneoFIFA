@page
@model TorneoDeFutbol.App.Frontend.pages.Directores.DetailsModel
@{
}
<h1>Detalles del Director Técnico</h1>
<table class="table table-dark">
    <tr>
            <th scope="col">Nombres</th>
            <td>@Model.director_tecnico.nombre</td>
        </tr>
        <tr>
            <th scope="col">Apellidos</th> 
            <td>@Model.director_tecnico.apellido</td>   
        </tr>
        <tr>
            <th scope="col">Numero de Documento</th> 
            <td>@Model.director_tecnico.numDocumento</td>
        </tr>
        <tr>
            <th scope="col">Años de Experiencia</th>
            <td>@Model.director_tecnico.aniosExperiencia</td>
        </tr>
        <tr>
            <th scope="col">Fecha de Nacimiento</th>
            <td>@Model.director_tecnico.fechaNacimiento</td>
        </tr>
        <tr>
            <th scope = "col">Género</th>
            <td>@Model.director_tecnico.genero</td>
        </tr>
        <tr>
            <th scope = "col">Ciudad</th>
            <td>@Model.director_tecnico.ciudad</td>
        </tr> 
        <tr>
            <th scope = "col">Dirección</th>
            <td>@Model.director_tecnico.direccion</td>
        </tr> 
        <tr>
             <th scope="col">Teléfono</th>
             <td>@Model.director_tecnico.numTelefono</td>   
        </tr>
</table>
<a class="btn btn-primary" asp-page="./Index">Cancelar</a>
